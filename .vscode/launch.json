  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome against localhost",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:4000",
      "webRoot": "${workspaceFolder}/nerfstudio/viewer/app"
    },
    {
      "name": "Python: Attach using Process ID",
      "type": "python",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: train",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        "--viewer.no-enable",
        "--logging.event-writer=wandb"
      ]
    },
    {
      "name": "Python: train ngp",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": ["instant_ngp"]
    },
    {
      "name": "Python: train nerfacto",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "nerfacto",
        // "--experiment_name=nerfstudio_experiment",
        "--logging.local-writer.max-log-size=0",
        "--data",
        "data/nerfstudio/dozer"
        // viewer
        // "--viewer.enable",
        // "--viewer.zmq-url=tcp://127.0.0.1:6001",
        // "--viewer.websocket-port=8014",
        // "--pipeline.model.randomize-background",
        // "--pipeline.model.cone-angle=0.00390625",
        // "nerfstudio-data"
      ]
    },
    {
      "name": "Python: train nerfacto colmap",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "nerfacto",
        // "--experiment_name=nerfstudio_experiment",
        "--logging.local-writer.max-log-size=0",
        "--data",
        "/home/ozgun/research/data/12831741023324393102_2673_230_2693_230",
        "colmap",
        // viewer
        // "--viewer.enable",
        // "--viewer.zmq-url=tcp://127.0.0.1:6001",
        // "--viewer.websocket-port=8014",
        // "--pipeline.model.randomize-background",
        // "--pipeline.model.cone-angle=0.00390625",
        // "nerfstudio-data"
      ]
    },
    {
      "name": "Python: train neuralangelo",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "neuralangelo",
        // "--experiment_name=nerfstudio_experiment",
        "--logging.local-writer.max-log-size=0",
        "--data",
        "data/sdfstudio/sdfstudio-demo-data/dtu-scan65",
        // viewer
        // "--viewer.enable",
        // "--viewer.zmq-url=tcp://127.0.0.1:6001",
        // "--viewer.websocket-port=8014",
        // "--pipeline.model.randomize-background",
        // "--pipeline.model.cone-angle=0.00390625",
        // "nerfstudio-data"
      ]
    },
    {
      "name": "Python: train ngp nerfstudio",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        // "--experiment_name=nerfstudio_experiment",
        "--logging.local-writer.max-log-size=0",
        "--viewer.no-enable"
        // viewer
        // "--viewer.enable",
        // "--viewer.zmq-url=tcp://127.0.0.1:6001",
        // "--viewer.websocket-port=8014",
        // "--pipeline.model.randomize-background",
        // "--pipeline.model.cone-angle=0.00390625",
        // "nerfstudio-data"
      ]
    },
    {
      "name": "Python: train ngp-friends",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        "--viewer.enable",
        "--viewer.zmq-url=tcp://127.0.0.1:6001",
        "--viewer.websocket-port=8014",
        "--steps_per_save=1000000",
        "--steps_per_test=1000000",
        "--pipeline.model.density-field-params.base-scale=4",
        "--pipeline.model.density-field-params.num-cascades=1",
        "pipeline.datamanager.train-dataparser:friends-data-parser-config",
        "--pipeline.datamanager.train-dataparser.scene-scale=4"
      ]
    },
    {
      "name": "Python: train semantic_nerfw",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": ["semantic_nerf"]
    },
    {
      "name": "Python: NeuS on Replica",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "neus",
        "--pipeline.model.sdf-field.inside-outside=True",
        "--pipeline.model.mono-depth-loss-mult=0.1",
        "--pipeline.model.mono-normal-loss-mult=0.05",
        "--vis=wandb",
        "sdfstudio-data",
        "--data=data/sdfstudio-demo-data/replica-room0",
        "--include_mono_prior=True"
      ]
    },
    {
      "name": "Python: NeuS-facto on Replica",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "neus-facto",
        "--pipeline.model.sdf-field.inside-outside=True",
        "--pipeline.model.mono-depth-loss-mult=0.1",
        "--pipeline.model.mono-normal-loss-mult=0.05",
        "--vis=wandb",
        "sdfstudio-data",
        "--data=data/sdfstudio-demo-data/replica-room0",
        "--include_mono_prior=True"
      ]
    },
    {
      "name": "Python: NeuS-facto on DTU (no mono-prior)",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "neus-facto",
        "--pipeline.model.sdf-field.inside-outside=False",
        "--vis=wandb",
        "sdfstudio-data",
        "--data=data/sdfstudio-demo-data/dtu-scan65",
        "--include_mono_prior=False",
        "--auto-orient=True"
      ]
    },
    {
      // Original nerfstudio command is as below:
      // ns-train splatfacto --pipeline.model.cull_alpha_thresh=0.005 --pipeline.model.continue_cull_post_densification=False
      // --pipeline.model.use-scale-regularization=True --data /home/ubuntu/data/gaussianpro_test colmap
      "name": "Python: train splatfacto nerfstudio",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "cwd": "/home/ubuntu/nerfstudio",
      "python": "/home/ubuntu/miniconda3/envs/nerfstudio/bin/python",
      "console": "integratedTerminal",
      "args": [
        "splatfacto",
        "--logging.local-writer.max-log-size=0",
        "--pipeline.model.cull_alpha_thresh=0.005",
        "--pipeline.model.continue_cull_post_densification=False",
        "--pipeline.model.use-scale-regularization=True",
        "--data", "/home/ubuntu/data/gaussianpro_test", "colmap"
      ]
    },
    {
      // Original nerfstudio command is as below:
      // ns-train splatfacto --pipeline.model.cull_alpha_thresh=0.005 --pipeline.model.continue_cull_post_densification=False
      // --pipeline.model.use-scale-regularization=True --data /home/ubuntu/data/gaussianpro_test colmap
      "name": "Python: train splatprofacto nerfstudio",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "cwd": "/home/ubuntu/nerfstudio",
      "python": "/home/ubuntu/miniconda3/envs/nerfstudio/bin/python",
      "console": "integratedTerminal",
      "args": [
        "splatprofacto",
        "--pipeline.model.cull_alpha_thresh=0.005",
        "--pipeline.model.continue_cull_post_densification=False",
        "--pipeline.model.use-scale-regularization=True",
        "--pipeline.model.propagated_iteration_begin=1",
        "--pipeline.model.propagated_iteration_after=1000",
        "--pipeline.model.depth_loss=True",
        "--pipeline.model.propagated_iteration_begin=10",
        "--pipeline.model.propagated_iteration_after=12000",
        "--pipeline.model.depth_error_min_threshold=0.8",
        "--pipeline.model.depth_error_max_threshold=1.0",
        "--pipeline.model.propagation_interval=30",
        "--pipeline.model.patch_size=20",
        "--pipeline.model.depth_max=80",
        "--pipeline.model.return_normal=True",
        "--pipeline.model.normal_loss=True",
        "--pipeline.model.sparse_loss=True",
        "--pipeline.model.flatten_loss=True",
        "--pipeline.model.lambda_l1_normal=0.001", 
        "--pipeline.model.lambda_cos_normal=0.001",
        "--data", "/home/ubuntu/data/gaussianpro_test", 
        "colmap", 
        "--auto-scale-poses", "False",
      ]
    },
  ]
}
